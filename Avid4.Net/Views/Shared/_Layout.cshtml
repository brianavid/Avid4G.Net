<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name = 'viewport' content = 'width=device-width, initial-scale=1, user-scalable=0'>
    <meta name='apple-mobile-web-app-capable' content='yes' />
    <meta name='apple-mobile-web-app-status-bar-style' content='black' />
    <title>Avid4</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    <link rel="SHORTCUT ICON" href="@Url.Content("/Content/favicon.ico")"/>
    <link rel="apple-touch-icon" href="/Content/apple-touch-icon.png" />
    <link rel="icon" href="/Content/Avid4-192.png" sizes="192x192" />
</head>
<body>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    <script src='~/Scripts/touch-emulator.js' type='text/javascript'></script>
    <script src='~/Scripts/jquery.hammer.min.js' type='text/javascript'></script>
    <script src='~/Scripts/jquery.nouislider.js' type='text/javascript'></script>
    <script src='~/Scripts/jquery.easing.min.js' type='text/javascript'></script>
    @Html.Raw(Render.Script("functions", Request))
    @RenderSection("scripts", required: false)
</body>
</html>
